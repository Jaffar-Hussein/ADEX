   "source": [
    "eqs = \"\"\"\n",
    "dv/dt = (1-v)/tau : 1\n",
    "\"\"\"\n",
    "\n",
    "G = NeuronGroup(1,eqs,method='euler',threshold = 'v > 0.8',reset = 'v=0')\n",
    "M = StateMonitor(G,'v',record=0)\n",
    "# G.v = 30\n",
    "\n",
    "run(60*ms)\n",
    "\n",
    "plt.plot(M.t/ms,M.v[0])\n",
    "plt.show()"
   ]
